{
    "name": "rezozero/renzo",
    "version": "0.0.1",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "rezozero/SLIR",
                "version": "2.0.4",
                "source": {
                    "url": "https://github.com/rezozero/SLIR.git",
                    "type": "git",
                    "reference": "rewrite/PSR-4"
                },
                "autoload": {
                    "psr-4":{"SLIR\\" : "core/"}
                }
            }
        }
    ],
    "require": {
        "rezozero/SLIR": "*",
        "doctrine/orm": "2.4.*",
        "symfony/yaml": "2.*",
        "symfony/console": "2.5.0",
        "symfony/http-foundation": "2.5.*",
        "symfony/routing": "2.5.*",
        "symfony/config": "2.5.*",
        "symfony/http-kernel": "2.5.*",
        "symfony/stopwatch": "2.5.*",
        "symfony/form": "2.5.*",
        "symfony/validator": "2.5.*",
        "symfony/security-csrf": "2.5.*",
        "symfony/twig-bridge": "2.5.*",
        "twig/twig":"1.*"
    },
    "autoload": {
        "psr-4": {
        	"RZ\\": "src/",
            "Themes\\": "themes/",
        	"GeneratedNodeSources\\": "sources/GeneratedNodeSources",
            "SLIR\\" : "vendor/rezozero/SLIR/core/"
        }
    }
}