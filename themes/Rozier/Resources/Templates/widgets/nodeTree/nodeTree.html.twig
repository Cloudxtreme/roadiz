{% if nodeTree %}
<nav class="nodetree-widget">
	<ul class="root-tree uk-nestable" data-uk-nestable="{'group':'nodeTree'}" data-parent-node-id="{{ node.getId }}">
		{% for cnode in nodeTree.getNodes %}
			{% include 'widgets/nodeTree/singleNode.html.twig' with { "node":cnode, "nodeTree":nodeTree } only %}
		{% endfor %}
	</ul>
</nav>
{% endif %}