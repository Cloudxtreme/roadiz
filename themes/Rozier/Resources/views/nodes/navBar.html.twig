<nav class="node-navbar-navigation uk-navbar">
	{% spaceless %}
	<ul class="uk-navbar-nav">
		<li class="uk-navbar-item{% if current == 'source' %} uk-active{% endif %}">
			<a class="uk-navbar-link" href="{{ path('nodesEditSourcePage', { nodeId: node.getId, translationId:translation.getId }) }}">{% trans %}node.content{% endtrans %}</a>
		</li>
		<li class="uk-navbar-item{% if current == 'edit' %} uk-active{% endif %}" >
			<a class="uk-navbar-link" href="{{ path('nodesEditPage', { nodeId: node.getId }) }}">{% trans %}node.parameters{% endtrans %}</a>
		</li>
		<li class="uk-navbar-item{% if current == 'tags' %} uk-active{% endif %}">
			<a class="uk-navbar-link" href="{{ path('nodesEditTagsPage', { nodeId: node.getId}) }}">{% trans %}tags{% endtrans %}</a>
		</li>
		<li class="uk-navbar-item{% if current == 'seo' %} uk-active{% endif %}">
			<a class="uk-navbar-link" href="{{ path('nodesEditSEOPage', { nodeId: node.getId, translationId:translation.getId}) }}">{% trans %}seo{% endtrans %}</a>
		</li>
		{% if node.isHidingChildren %}
		<li class="uk-navbar-item{% if current == 'tree' %} uk-active{% endif %}">
			<a class="uk-navbar-link" href="{{ path('nodesTreePage', { nodeId: node.getId}) }}">{% trans %}tree{% endtrans %}</a>
		</li>
		{% endif %}
	</ul>
	{% endspaceless %}
	<div class="uk-navbar-content">
		<a class="uk-button" href="{{ path('nodesHomePage') }}"><i class="uk-icon-reply"></i> {% trans %}back_to.nodes{% endtrans %}</a>
	</div>
</nav>
