{% extends 'base.html.twig' %}

{% block content %}
<section class="content-global import-node-types">

    <header class="content-header header-node-types header-node-types-import">
         <h1 class="content-title node-types-export-title">{% trans %}import.nodeType{% endtrans %}</h1>
    </header>

    <article class="content content-node-types-import content-no-action-menu">
        {% form_theme form 'forms.html.twig' %}
        <form class="uk-form uk-form-stacked" action="#" method="post" {{ form_enctype(form) }}>
            {{ form_widget(form) }}
            <button type="submit" class="uk-button uk-button-primary"><i class="uk-icon-upload"></i> {% trans %}import{% endtrans %}</button>
            <a href="{{ path('nodeTypesHomePage') }}" class="uk-button"><i class="uk-icon-reply"></i> {% trans %}cancel{% endtrans %}</a>
        </form>
    </article>

</section>
{% endblock %}