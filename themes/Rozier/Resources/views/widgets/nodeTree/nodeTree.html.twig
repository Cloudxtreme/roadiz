{% if nodeTree %}
<nav class="nodetree-widget" {% if head.ajax %}style="display:none;"{% endif %}">
    {% if mainNodeTree %}
    <h4>{% trans %}Node-tree{% endtrans %}</h4>
    {% endif %}
	<ul class="root-tree uk-nestable" data-uk-nestable="{'group':'nodeTree'}" data-parent-node-id="{{ node.getId }}">
		{% for cnode in nodeTree.getNodes %}
			{% include 'widgets/nodeTree/singleNode.html.twig' with { "node":cnode, "nodeTree":nodeTree, "securityContext":securityContext} only %}
		{% endfor %}
	</ul>
</nav>
{% endif %}
