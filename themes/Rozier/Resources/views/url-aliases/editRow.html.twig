<tr class="{% if loop.index0 is odd %}odd{% endif %}">
    <td>
        {# Update URL alias #}
        {% form_theme urlAlias.editForm '@Rozier/horizontalForms.html.twig' %}
        <form class="uk-form uk-form-horizontal urlalias-form" action="#" method="post" {{ form_enctype(urlAlias.editForm) }}>
            {{ form_widget(urlAlias.editForm) }}
            <button class="uk-button uk-button-small uk-button-primary" type="submit" value="{% trans %}edit{% endtrans %}"><i class="uk-icon-pencil"></i> {% trans %}edit{% endtrans %}</button>
        </form>
    </td>
    <td>{{ urlAlias.alias.getNodeSource.getTranslation.getName }}</td>
    <td>
        {# Delete URL Alias #}
        {% form_theme urlAlias.deleteForm '@Rozier/horizontalForms.html.twig' %}
        <form class="uk-form" action="#" method="post" {{ form_enctype(urlAlias.deleteForm) }}>
            {{ form_widget(urlAlias.deleteForm) }}
            <button class="uk-button uk-button-small uk-button-danger" type="submit" value="{% trans %}delete{% endtrans %}"><i class="uk-icon-rz-trash-o"></i> {% trans %}delete{% endtrans %}</button>
        </form>
    </td>
</tr>
